
view {
    color: #333;
    font-size: 28rpx;

    line-height: 42rpx
}





view {
    box-sizing: border-box;
}

.f_cb:after,
.f_cbli li:after {
    display: block;
    content: ".";
    clear: both;
    visibility: hidden;
    height: 0;
    overflow: hidden
}

.page {
    min-height: 100%;
}

.oh {
    overflow: hidden
}

.page .oh {
    overflow: hidden
}

.pl {
    position: relative
}

.pa {
    position: absolute
}

.pf {
    position: fixed
}

.fl {
    float: left
}

.fr {
    float: right
}

.db {
    display: block
}

.dbi {
    display: inline-block
}

.dn {
    display: none
}

.tc {
    text-align: center
}

.tr {
    text-align: right
}

.tl {
    text-align: left
}


.c2 {
    color: #f55
}


.cal_mn { overflow: hidden; }
.sunday { color: #f55; }
.cal .wk { display: flex; width: 100%; height: 36px; z-index: 2; background: #e5e5e5;}
.cal .wk view { flex: 1; line-height: 36px;}
.cal .plc { padding-top: 36px; padding-bottom: 24px; background: #ffffff;}
.cal .mth { height: 44px; line-height: 44px;background: #f4f4f4;}
.cal .cic { height: auto; }
.cal .ej { width: 14.2857143%; height: 48px; }
.ej .day { line-height: 48px; }
.ej .start, .ej .end { line-height: 16px; padding-top: 8px; padding-bottom: 24px; background-color: #fb5f0a; color: #fff; }
.ej .start { border-top-left-radius: 20px; border-bottom-left-radius: 20px; }
.ej .end { border-top-right-radius: 20px; border-bottom-right-radius: 20px; }
.ej .info { position: absolute; z-index: 99; left: 50%; margin-left: -10px; color: #fff; font-size: 11px; bottom: 2px; }
.ej .range { background-color: #fedfce; }
.cic .ej .range { display: block; color: #fb5f0a; }
.ej .range .info { color: #09bb07; }
.ej .enable { color: #333; }
.ej view { color: #b2b2b2; }
.ej .sunday { color: red; }
.vfd { width: 100%; height: 30px; }
