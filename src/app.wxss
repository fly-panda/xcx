/**app.wxss**/
/*公用样式*/
page{
  font-family:Tahoma,Arial,Helvetica,sans-serif;
  background: #f4f4f4;
  font-size: 14px;
  color:#353535;
}
.highlight{
  color:#fc8f53;
}
.clear::after {
  clear: both;
  content: ".";
  display: block;
  height: 0;
  overflow: hidden;
  visibility: hidden;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:22px;
  margin-right: 6px;
  color:#888888;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.txtOrange{
  color:#fb5f0a;
}
.txtLight{
  color: #b2b2b2;
}
.largeBtn{
  margin: 30rpx;
}
.eBtn{
  display:inline-block;
  font-size: 14px;
  height:36px;
  line-height:36px;
  margin-right: 0;
}
.btnPrimary{
  background: #fb5f0a;
  color:#ffffff;
  border:1rpx solid #fb5f0a;
}
.btnCancel{
  background: #fff;
  color: #353535;
  border:1rpx solid #999;
}
.press{
  opacity: 0.5;
}
.disable{
  opacity: 0.2;
}
.flexRow{
  display: flex;
  flex-direction: row;
}
.between{
  justify-content: space-between;
}
.center{
  align-items: center;
}
.flexColumn{
  display: flex;
  flex-direction:column;
}
.arrow{
  font-size: 14px;
  color: #b2b2b2;
  margin-right: 0;
}

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}


